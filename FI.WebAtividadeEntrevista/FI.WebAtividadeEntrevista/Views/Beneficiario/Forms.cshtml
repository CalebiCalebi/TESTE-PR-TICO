
<form id="formCadastro" method="post">
    <div class="row">
        <div class="col-md-5">
            <div class="form-group">
                <label for="Nome">Nome:</label>
                <input required="required" type="text" class="form-control" id="Nome" name="Nome" placeholder="Ex.: João" maxlength="50">
            </div>
        </div>
        <div class="col-md-5">
            <div class="form-group">
                <label for="CPF">CPF:</label>
                <input required="required" type="text" class="form-control" id="CPF" onblur="validarCPF(this);" name="CPF" placeholder="Ex.: 010.011.111-00" maxlength="14">
            </div>
        </div>
        <div class="pull-right">
            @Html.ActionLink("Incluir", "Incluir", "Beneficiario", new { @class = "btn btn-sm btn-success pull-right" })
        </div>

    </div>

</form>
